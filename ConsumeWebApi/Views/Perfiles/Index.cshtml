@model IEnumerable<ConsumeWebApi.Models.Perfiles>

@{
    ViewBag.Title = "Index";
}

<h2>Gestion de Perfiles</h2>

<p>
    @Html.ActionLink("Crear Nuevo Perfil", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion_Perfil)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion_Perfil)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Codigo_Perfil }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Codigo_Perfil }) |
            @Html.ActionLink("Deshabilitar", "Delete", new { id=item.Codigo_Perfil })
        </td>
    </tr>
}

</table>
